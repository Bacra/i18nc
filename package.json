{
  "name": "i18nc-core",
  "version": "4.2.2",
  "description": "I18N Tool for JS files",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm ls",
    "test": "cross-env DEBUG=i18nc-core* mocha test/test_*",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha -- test/test_* --reporter dot",
    "test-travis": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- test/test_* --reporter dot",
    "test-build": "cross-env TEST_BUILD=true mocha test/test_*"
  },
  "dependencies": {
    "debug": "^2.3.0",
    "escodegen": "^1.8.1",
    "esmangle": "^1.0.1",
    "esprima": "^3.1.3",
    "extend": "^3.0.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "cross-env": "^2.0.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Bacra/node-i18nc-core.git"
  },
  "keywords": [
    "i18n",
    "translate",
    "extract",
    "Chinese",
    "esprima",
    "ast"
  ],
  "author": "Bacra Woo<bacra.woo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bacra/node-i18nc-core/issues"
  },
  "homepage": "https://github.com/Bacra/node-i18nc-core"
}
